<?xml version="1.0" encoding="UTF-8"?>


<?import com.jfoenix.controls.JFXToggleButton?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<StackPane
  xmlns:fx="http://javafx.com/fxml/1"
  minHeight="400.0" minWidth="400" prefHeight="400.0" prefWidth="400.0"
  xmlns="http://javafx.com/javafx/11"
  fx:controller="com.unclezs.novel.app.main.ui.pages.home.views.SearchNovelView"
  fx:id="root">
  <VBox fx:id="box">
    <Button text="show=false" fx:id="showFalse"/>
    <Button text="清空" fx:id="clear"/>
    <HBox>
      <Button text="录入舞台热键" fx:id="window"/>
      <Button text="取消舞台热键" fx:id="cancelWindow"/>
    </HBox>
    <HBox>
      <Button text="录入全局热键" fx:id="global"/>
      <Button text="取消全局热键" fx:id="cancelGlobal"/>
    </HBox>
    <JFXToggleButton text="启动调试模式" fx:id="debug"/>
    <Text fx:id="text"/>
  </VBox>
</StackPane>
